! xterm ----------------------------------------------------------------------

!general settings
XTerm*termName:         xterm
XTerm*geometry:         80x50
XTerm*scrollBar:        false
XTerm*scrollTtyOutput:  false
XTerm*scrollKey:        true
XTerm*eightBitInput:    true
XTerm*eightBitOutput:   true
XTerm*locale:           true
XTerm*utf8:             2
XTerm*utf8Fonts:        true
XTerm*utf8Title:        true

!font settings
XTerm*renderFont:       true
XTerm*faceName:         DejaVu Sans Mono:size=9
!XTerm*faceName:         Ubuntu Mono:size=10
XTerm*allowBoldFonts:   true
XTerm*colorBDMode:      false
XTerm*colorBLMode:      false
XTerm*colorITMode:      false
XTerm*colorRVMode:      false
XTerm*colorULMode:      false
XTerm*underLine:        true

!colour settings
XTerm*background:       #000000
